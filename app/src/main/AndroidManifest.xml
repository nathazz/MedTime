<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher_medtime"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_medtime_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MedTime"
        tools:targetApi="31">
        <activity
            android:name="com.example.medtime.controller.MedCadastrados"
            android:exported="false" />
        <activity
            android:name="com.example.medtime.controller.NovoMedicamentoActivity"
            android:exported="false" />
        <activity
            android:name="com.example.medtime.controller.MedcadastradosActivity"
            android:exported="false" />
        <activity
            android:name="com.example.medtime.controller.AgendamentosActivity"
            android:exported="false" />
        <activity
            android:name="com.example.medtime.controller.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.AgendamentoActivity" />
        <activity android:name="com.example.medtime.controller.CadastrarActivity" />
    </application>

</manifest>